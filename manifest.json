{
  "manifest_version": 2,
  "name": "Copy Store",
  "version": "0.1",
  "author": "ductbm",
  "description": "Sản phẩm đầu tay của Đức tbm...",
  "permissions": [
    ""
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "logo.png",
    "default_popup": "index.html",
    "default_title": "Click here Tô Đức."
  }
}
